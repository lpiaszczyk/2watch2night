<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.DrawerLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/movie_discover_activity_drawer_layout"
    tools:openDrawer="start">
    <FrameLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/movie_discover_activity_frame_layout"/>

    <include layout="@layout/main_navigation_view"
        android:id="@+id/movie_discover_activity_main_navigation_drawer"/>
</android.support.v4.widget.DrawerLayout>